package inflearn_study_algorithm;

import java.util.LinkedList;
import java.util.Queue;
import java.util.Scanner;

public class Stack_Queue_07 {

	public static void main(String[] args) {
		
		/*
		* ABOUT
		*
		* DATE: 2022-10-
		* AUTHOR: dms873
		*
		* TITLE: 교육과정 설계      
		*
		* COMMENT: 
		* 
		**/
		
		Scanner sc = new Scanner(System.in);
		// System.out.print("필수 과목 : ");
		String sub = sc.nextLine();
		// System.out.print("수업 설계 : ");
		String cl = sc.nextLine();
		sc.close();
		
		Queue<Character> q = new LinkedList<>();
		for(int i = 0; i < sub.length(); i++) {
			q.add(sub.charAt(i));
		}
		System.out.println(q);
		
		String result = "YES";
		
		for(int i = 0; i < cl.length(); i++) {
			System.out.println("cl : " + cl.charAt(i));
			System.out.println("q : " + q.peek());
			if(cl.charAt(i) == q.peek()) {
				q.remove();
				if(q.isEmpty()) break;
			}
			else if(q.isEmpty() && cl.charAt(i) != q.peek()) {
				result = "NO";
				break;
			}
		}
		System.out.println(result);
	}
}
